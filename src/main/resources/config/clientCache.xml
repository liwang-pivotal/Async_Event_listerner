<!DOCTYPE client-cache PUBLIC 
"-//GemStone Systems, Inc.//GemFire Declarative Caching 7//EN" 
"http://www.gemstone.com/dtd/cache7_0.dtd">

<client-cache>

    <pool name="clientPool" subscription-enabled="true">
        <locator host="localhost" port="10334"/>
    </pool>
    
    <pdx read-serialized="true">
        <pdx-serializer>
            <class-name>com.gemstone.gemfire.pdx.ReflectionBasedAutoSerializer</class-name>
            <parameter name="classes">
                <string>io.pivotal.domain.*</string>
            </parameter>
        </pdx-serializer>
    </pdx>
    
    <region name="UnitTelemetry">
        <region-attributes refid="PROXY" pool-name="clientPool" />
    </region>
    
    <region name="UnitTelemetryHelper">
        <region-attributes refid="PROXY" pool-name="clientPool" />
    </region>
    
    <region name="DummyRegion">
        <region-attributes refid="PROXY" pool-name="clientPool" />
    </region>

</client-cache>